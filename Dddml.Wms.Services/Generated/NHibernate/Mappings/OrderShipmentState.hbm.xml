<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
				   assembly="Dddml.Wms.Common" 
				   namespace="Dddml.Wms.Domain.OrderShipment">
  
